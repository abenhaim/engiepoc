<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
  </startup>
  <runtime>
    <gcServer enabled="true"/>
  </runtime>
  <appSettings>
    <add key="StorageConnectionString" value="" />

    <add key="EndPointUrl" value="https://<account>.documents.azure.com:443/"/>
    <add key="AuthorizationKey" value=""/>
    <add key="DatabaseName" value="db"/>
    <add key="CollectionName" value="ingestion"/>
      
    <!-- determined by the tool -->
    <add key="DegreeOfParallelism" value="-1"/>

    <add key="CollectionPartitionKey" value="/partitionKey"/>
  </appSettings>
</configuration>